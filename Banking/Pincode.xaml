        <TextBox x:Name="_3" HorizontalAlignment="Left" Margin="265,142,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="66" Height="51" TextAlignment="Center"/>
        <TextBox x:Name="_4" HorizontalAlignment="Left" Margin="353,142,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="66" Height="51" TextAlignment="Center"/>
        <TextBox x:Name="_5" HorizontalAlignment="Left" Margin="438,142,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="66" Height="51" TextAlignment="Center"/>
        <TextBox x:Name="_6" HorizontalAlignment="Left" Margin="523,142,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="66" Height="51" TextAlignment="Center"/>
        <Button x:Name="subbtn" Content="SUBMIT" Margin="215,231,420,162" Click="Button_Click"/>
    </Grid>
</Window>
